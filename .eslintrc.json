{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": "off",
        "eqeqeq": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "linebreak-style": ["error", "windows"],
        "no-param-reassign": "off",
        "no-useless-catch": "off"
    }
}
